{
  "version": 2,
  "builds": [
      { "src": "package.json", "use": "@vercel/static-build" },
      { "src": "Backend/app.py", "use": "@vercel/python" }
  ],
  "routes": [
      { "src": "/contact", "dest": "/Backend/app.py" },
      { "src": "/contact", "dest": "./src/app/domains/components/contact-me/contact-me.component.html" },
    { "src": "/about", "dest": "./src/app/domains/components/about-me/about-me.component.html" },
    { "src": "/projects", "dest": "./src/app/domains/components/projects/projects.component.html" },
    { "src": "/(.*)", "dest": "/portafolio/browser/index.html" },
      { "src": "/(.*)", "dest": "/portafolio/browser/$1" }
  ]
}