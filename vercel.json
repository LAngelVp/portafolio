{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "Backend/app.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/Backend/app.py" },
    { "src": "/(.*)", "dest": "/portafolio/browser/", "continue": true }
  ]
}