  {
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" },
      { "src": "Backend/app.py", "use": "@vercel/python" }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/Backend/app.py" },
      { "source": "/(.*)", "destination": "/portafolio/browser/index.html" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/Backend/app.py" },
      { "src": "/(.*)", "dest": "/portafolio/browser/$1" } 
    ]
  }
  